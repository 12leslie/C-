using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System. Threading.Tasks;

namespace estructuraRepetitivaWhile9
{
    class Program
    {
        static void  Main (string[]args)
        {
            Int mult8;
            mult8=8;
            while ( mult8<=500)
            {
                Console.Write( mult8);
                Console.Write("-");
                mult8=mult8 +8;

            }
            Console.ReadKey();

        }
    }
}